or:$bg-gray;\n    background-color: $bg-gray;\n    padding: 4rem 3rem 1rem 3rem;\n\n    box-sizing:border-box;\n}\n\n.intro-title{\n    font-family: 'font','Helvetica Neue', arial, sans-serif;\n    font-size: 3rem;\n    margin-bottom: 1em;\n    line-height: 1;\n    width: 100%;\n    @media only screen and (max-width: 1400px) {\n        width: 700px;\n    }\n    @media only screen and (max-width: 945px) {\n        width: 100%;\n    }\n}\n\n.intro-text{\n    max-width: 1000px;\n    @media only screen and (max-width: $secondbreak) {\n        max-width: 100vw;\n    }\n}\n\n.intro-buttons{\n    width: 100%;\n    margin-bottom: 3rem;\n    display:flex;\n}\n// –––––––––––––– intro - right\n\n.intro-container-right{\n    width: 270px;\n    height: auto;\n    position: absolute;\n    top: 0;\n    right: 0;\n\n    padding: 4rem 2rem 0rem 1rem;\n    box-sizing: border-box;\n\n    @media only screen and (max-width: $firstbreak) {\n        width: 230px;\n        padding: 2rem 2rem 0rem 1rem;    \n    }\n    @media only screen and (max-width: 950px) {\n        width: 350px;\n        padding: 3rem 3rem 0rem 1rem;    \n        display: flex;\n    }\n}\n\n.intro-logo{\n    max-width: 100%;\n    margin-bottom: 1.5rem;\n    @media only screen and (max-width: $firstbreak) {\n        margin-bottom: 1rem;  \n    }\n    @media only screen and (max-width: 950px) {\n        max-width: 50%;\n        box-sizing:border-box;\n        padding-left: 0.5rem;\n    }\n}\n\n","style/variables.scss":"$lh: 1em;\n$bg1: rgb(182, 222, 179);\n$bg2: rgb(182, 182, 182);\n$bg3: rgb(146, 139, 244);\n\n$bg-gray: rgb(240, 240, 240);\n$bg-gray-light: rgb(230, 230, 230);\n$bg-gray-medium: rgb(210, 210, 210); \n$bg-gray-dark: rgb(150, 150, 150);\n$bg-green: #505f47;\n$bg-red: #f6d8d8;\n\n$links: rgb(126, 159, 123);\n\n\n$firstbreak: 1500px;\n$secondbreak: 1300px;\n\n\n$left-timeline-margin: 0.8rem;\n\n$radius1: 1rem;\n\n\n$gray1: rgb(240, 240, 240);\n$gray2: rgb(230, 230, 230);\n$gray3: rgb(220, 220, 220);\n$gray4: