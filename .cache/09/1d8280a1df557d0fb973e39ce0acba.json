 width: auto;\n\n    border-radius: 2rem;\n    @media screen and (max-width: $medium-tablet-break) {\n        margin: 1vw 2vw;\n        padding: 2.5rem 3vw;\n        border-radius: 4vw;\n    }\n    \n}\n\n// ------------ TITLE\n\n.timeline-section-title-container {\n    width: 100%;\n    text-align: center;\n}\n\n.timeline-section-title {\n    font-size: 1.5rem;\n}\n.timeline-section-subtitle {\n    font-size: 0.8rem;\n}\n\n// ------------ tiles [] [] []\n\n.timeline-section-years-container {\n    margin-left: auto;\n    margin-right: auto;\n\n    display: flex;\n    flex-wrap: wrap;\n    max-width: 1200px;\n}\n\n.year-title {\n    padding-left: 0.5rem;\n    padding-top: 2rem;\n    padding-bottom: 1rem;\n    @media screen and (max-width: $medium-tablet-break) {\n        padding-top: 1rem;\n        padding-bottom: 0.5rem;\n    }\n}\n\n.tiles-container {\n    display: flex;\n    align-items: flex-start;\n    height: auto;\n\n    background-color: white;\n}\n\n.tiles-subcontainer {\n    display: flex;\n    align-items: flex-start;\n    height: auto;\n}\n\n.tile-wrap {\n    width: calc((100vw - 7rem) / 3);\n    max-width: 400px;\n    height: 100%;\n    position: relative;\n    @media screen and (max-width: $medium-tablet-break) {\n        width: calc((100vw - 4vw - 6vw) / 3);\n    }\n}\n\n.tile-image {\n    width: calc(100% - 5px);\n    max-width: 400px;\n    box-sizing: border-box;\n\n    height: auto;\n    border-radius: 1rem;\n    float: left;\n    @media screen and (max-width: $medium-tablet-break) {\n        border-radius: 2vw;\n        width: calc(100% - 2px);\n    }\n}\n\n.tile-label {\n    display: flex;\n    text-align: center;\n    align-items: center;\n    justify-content: center;\n    color: white;\n    font-size: 1rem;\n\n    background-color: rgba(0, 0, 0, 0.5);\n\n    position: absolute;\n\n    visibility: hidden;\n    opacity: 0;\n\n    user-select: none;\n\n    width: calc(100% - 5px);\n    max-width: 400px;\n    padding-right: 5px;\n    box-sizing: border-box;\n    height: 100%;\n\n    border-radius: 1rem;\n\n    white-space: pre-line;\n\n    transition: 0.2s;\n    @media screen and (max-width: $medium-tablet-break) {\n        width: calc(100% - 2px);\n        border-radius: 2vw;\n        font-size: 0.8rem;\n        line-height: 1.1;\n    }\n}\n\n.tile-wrap:hover .tile-label{\n    visibility: visible;\n    opacity: 1;\n}\n\n.tile-wrap:hover .plus{\n    visibility: visible;\n    opacity: 1;\n}\n\n.tile-wrap:hover {\n    cursor: pointer;\n}\n\n.tile-wrap-none:hover {\n    cursor: default !important;\n}\n\n.plus{\n    visibility: hidden;\n    opacity: 0;\n    position: absolute;\n    top: 1.5vw;\n    right: 1.5vw;\n    width: 1.5vw;\n    height: 1.5vw;\n    background-image: url(\"../../public/ui/plus.svg\"); \n    // filter: drop-shadow(0 0 10px rgba(0,0,0,0.5));\n    z-index: 100;\n\n    @media screen and (max-width: $large-tablet-break) {\n        width: 2.5vw;\n        height: 2.5vw;\n    }\n    @media screen and (max-width: $medium-tablet-break) {\n        width: 1rem;\n        height: 1rem;\n    }\n}"},"lineCount":null}},"error":null,"hash":"15286cddaa0c5eccbea97c3db2c33222","cacheData":{"env":{}}}