iginal":{"line":48,"column":4},"generated":{"line":188,"column":2}},{"source":"style/variables.scss","name":null,"original":{"line":2,"column":6},"generated":{"line":188,"column":21}},{"source":"style/style.scss","name":null,"original":{"line":49,"column":4},"generated":{"line":189,"column":2}},{"source":"style/style.scss","name":null,"original":{"line":49,"column":4},"generated":{"line":189,"column":58}},{"source":"style/style.scss","name":null,"original":{"line":50,"column":4},"generated":{"line":190,"column":2}},{"source":"style/style.scss","name":null,"original":{"line":50,"column":4},"generated":{"line":190,"column":17}},{"source":"style/style.scss","name":null,"original":{"line":54,"column":0},"generated":{"line":193,"column":0}},{"source":"style/style.scss","name":null,"original":{"line":55,"column":4},"generated":{"line":194,"column":2}},{"source":"style/style.scss","name":null,"original":{"line":55,"column":4},"generated":{"line":194,"column":59}}],"sources":{"style/header.scss":"\n.header-container-map {\n    position: fixed;\n    top: 15px;\n    right: 15px; \n    width: auto;\n    z-index: 100;\n\n    // background-color: rgba(146, 139, 244, 0.7);\n    backdrop-filter: blur(5px) brightness(90%) ;\n\n    padding: $lh/3 0 $lh/2 0;\n    border-radius: $lh*2;\n\n    // filter: drop-shadow(0px 0px 3px rgb(30, 30, 30));\n}\n\n.header-container-timeline {\n    position: fixed;\n    top: 15px;\n    right: 15px; \n    width: auto;\n    z-index: 100;\n\n    // background-color: rgba(146, 139, 244, 0.7);\n    padding: $lh/3 0 $lh/2 0;\n    border-radius: $lh*2;\n\n    // filter: drop-shadow(0px 0px 3px rgb(30, 30, 30));\n}\n\n.header-link-container{\n    width: auto;\n    float: right;\n    margin: 0 $lh;\n}\n\n.header-link{\n    display: inline-block;\n    cursor: pointer;\n    margin: 0 $lh;\n\n    font-size: 20px;\n\n    text-transform: lowercase ;\n    text-align: center;\n    letter-spacing: 2px;\n    font-family: 'font', sans-serif;\n\n    transition: 0.4s;\n}\n\n.header-link:hover{\n    opacity: 0.4;\n}","style/map.scss":"@import \"variables.scss\";\n\n\n.map-container{\n    cursor:initial;\n    height: 100vh;\n    width: 100vw;\n}\n\n.map-marker{\n    width:$lh*2;\n    height: $lh*2;\n    z-index: 100;\n\n    background-color: rgba(255, 255, 255, 50);\n    border-radius: $lh*2;\n    border: 2px $bg1 solid;\n    // filter: drop-shadow(0px 0px 3px rgb(100, 100, 100));\n    cursor: pointer;\n    transition: 0.3s;\n}\n\n.map-marker:hover{\n    transform: scale(1.1);\n}\n\n.map-dialog{\n    position: fixed;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%,-50%);\n\n    height: 80vh;\n    width: 800px;\n\n    background-color: white;\n    border-radius: $lh*1.5;\n    border: 2px $bg1 solid;\n\n    // filter: drop-shadow(0px 0px 10px rgb(100, 100, 100));\n\n\n\n    z-index: 100;\n}","style/timeline.scss":".timeline-container{\n    width: 100vw;\n    height: 100vh;\n\n    background: $bg2;\n}\n\n.timeline-spacer{\n    float: left;\n    width: 100px;\n    height: 100vh;\n}\n\n.timeline-decade{\n    padding-top: $lh;\n    width: 100vw;\n    height: 100vh;\n    float: left;\n\n    overflow-y: scroll;\n\n    font-size: 50px;\n    font-family: 'font', sans-serif;\n}\n\n.timeline-decade-last{\n    width: 30vw;\n}\n\n.timeline-year-first{\n    float:left;\n    width: 12vw;\n}\n\n.timeline-year-others{\n    float:left;\n    width: 9.6vw;\n    font-size: 1.5rem;\n    padding-top: 1rem;\n}","style/variables.scss":"$lh: 1em;\n$bg1: rgb(182, 222, 179);\n$bg2: rgb(182, 182, 182);\n$bg3: rgb(146, 139, 244);\n\n$links: rgb(126, 159, 123);\n\n","style/landing.scss":"@import \"variables.scss\";\n\n\n.landing-container{\n    width: 100%;\n    height: 100%;\n    margin: 0;\n}\n\n\n\n.landing-text{\n    padding-top: 80px;\n    padding-left: $lh;\n    padding-right: $lh;\n\n\n    font-family: 'font', serif;\n    font-size: 35px;\n    line-height: 1.1;\n\n    max-width: 1400px;\n}\n\n.landing-headline{\n    font-family: 'font', serif;\n    font-size: 70px;\n}\n\n.landing-map{\n    color: black;\n    cursor: pointer;\n    display: inline-block;\n    transition: 0.4s;\n    background-color: $links;\n    border-radius: $lh;\n    padding: $lh/4 $lh/2;\n    margin-right: $lh/2;\n}\n\n.landing-map:hover{\n    // text-decoration: none;\n    opacity: 0.5;\n}","style/style.scss":"@import \"variables.scss\";\n@import \"header.scss\";\n@import \"map.scss\";\n@import \"timeline.scss\";\n@import \"landing.scss\";\n\n@font-face {\n    font-family: 'font'; \n    src: url('../public/fonts/NeueMontreal-Regular.otf') format('opentype');\n    font-weight: normal;\n    font-style: normal;\n}\n\n@font-face {\n    font-family: 'fontL'; \n    src: url('../public/fonts/NeueMontreal-Light.otf') format('opentype');\n    font-weight: lighter;\n    font-style: normal;\n}\n\n@font-face {\n    font-family: 'fontM'; \n    src: url('../public/fonts/NeueMontreal-Medium.otf') format('opentype');\n    font-weight: normal;\n    font-style: normal;\n}\n\n\n#root {\n    width: 100%;\n    height: 100%;\n}\n\n.main-container {\n    width: 100%;\n    height: 100%;  \n}\n\nhtml, body {\n    overflow: hidden;\n    width: 100%;\n    height: 100%;\n    margin: 0;\n    padding: 0;\n    \n    text-rendering: geometricPrecision;\n    -webkit-font-smoothing: antialiased;\n    background: $bg1;\n    font-family: 'font','Helvetica Neue', arial, sans-serif;\n    font-size: 19px;\n\n}\n\n.h1{\n    font-family: 'fontL','Helvetica Neue', arial, sans-serif;\n}"},"lineCount":null}},"error":null,"hash":"dafafbdd8d3d310e1efaa6a8ce514e12","cacheData":{"env":{}}}